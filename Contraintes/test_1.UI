proc tab2interpolator2 {tab interpolator} {
 set nb_C [$tab get_nb_C]
 set nb_L [$tab get_nb_L]
 set L 0
 while {$L < $nb_L} {
   set C 1
   set v [$tab get_case [expr $L+1] 0]
   $v set_L_examples ""
   for {set C 1} {$C < $nb_C} {incr C} {
     set ref [$tab get_case $L $C]
	 set val [$tab get_case [expr $L+1] $C]
     if {![string equal $ref ""]} {$v Add_exemple $ref $val <=}
	 incr C
	 if {$C < $nb_C} {set ref [$tab get_case $L $C]} else {set ref ""}
    }
   incr L 2
  }
}

proc interpolator2tab {interpolator tab} {
# number of columns?
 set nb_max 0; set nb_L 0
 foreach infos_scalar [$interpolator get_L_infos_scalar] {
   set nb [$infos_scalar get_nb_max_example]
   set nb_max [expr $nb>$nb_max?$nb:$nb_max]
   incr nb_L [expr 2*[$infos_scalar get_nb_related_vars]]
  }
# set the size of tab (one more column for name of the variables)  
 $tab set_dims $nb_L [expr $nb_max+1]
 
# Fill the tab with informations
 set L 0
 foreach infos_scalar [$interpolator get_L_infos_scalar] {
   foreach v [$infos_scalar get_L_related_vars] {
     $tab set_case $L          0 $infos_scalar
	 $tab set_case [expr $L+1] 0 $v
	 set C 1; set L_e [$v get_L_examples]; set nb_e [llength $L_e]
     foreach e $L_e {
	   $tab set_case $L          $C [lindex $e 0]
	   $tab set_case [expr $L+1] $C [lindex $e 1]
       incr C
	  }
	 for {set i $nb_e} {$i < $nb_max} {incr i} {
	   $tab set_case $L          $i {}
	   $tab set_case [expr $L+1] $i {}
	  }
	 incr L 2
	}
  }
}

