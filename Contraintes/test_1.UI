proc Tab_del_line2interpolator {num tab interpolator} {
 set obj [$tab get_case $num 0]
 set Lc  [gmlObject info classes $obj]
 if {[lsearch $Lc B207_var_obj] != -1} {
   puts "$obj dispose"
   $obj dispose
  }
}
